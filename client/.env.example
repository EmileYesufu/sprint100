# Sprint100 Client Environment Configuration
# Copy this file to .env and update values as needed

# For Expo Go testing on iPhone (use your Mac's local IP)
# Find your IP: ipconfig getifaddr en0 (macOS) or ipconfig (Windows)
EXPO_PUBLIC_API_URL=http://192.168.1.250:4000

# Alternative: Server URL for development
SERVER_URL=http://localhost:4000

# Test server URL for external testers
TEST_SERVER_URL=https://sprint100-test.example.com

# App environment (development, test, production)
APP_ENV=development

# Optional: Enable verbose logging
REACT_NATIVE_APP_VERBOSE=false
