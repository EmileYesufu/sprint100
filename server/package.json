{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "postinstall": "prisma generate",
    "start:prod": "cross-env NODE_ENV=production node dist/server.js",
    "start:test": "cross-env NODE_ENV=test node dist/server.js",
    "start:ngrok": "npx localtunnel --port 4000",
    "seed:test": "ts-node seed/test-users.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node seed/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:dev": "docker-compose up -d",
    "docker:prod": "docker-compose -f deploy/docker-compose.prod.yml up -d",
    "docker:down": "docker-compose down",
    "lint": "tsc --noEmit",
    "migrate:staging": "dotenv -e ../.env.staging -- prisma migrate deploy",
    "migrate:prod": "dotenv -e ../.env.prod -- prisma migrate deploy",
    "seed:demo": "dotenv -e ../.env.staging -- ts-node seed/demo-seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.16.3",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prisma": "^6.16.3",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^24.7.0",
    "@types/supertest": "^2.0.16",
    "cross-env": "^7.0.3",
    "tsx": "^4.16.5",
    "diff": "^4.0.2",
    "jest": "^29.7.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "yn": "^3.1.1"
  },
  "prisma": {
    "seed": "ts-node seed/seed.ts"
  },
  "engines": {
    "node": ">=20"
  }
}
